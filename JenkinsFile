/* groovylint-disable CompileStatic */
pipeline {
    agent any
    stages {
        stage('CompileTypescript') {
            steps {
                sh 'tsc'
            }
        }
        stage('BuildApp') {
            steps {
                sh 'npm install'
            }
        }
        stage('Deploy') {
            steps {
                sh 'node .'
            }
        }
    }
}
